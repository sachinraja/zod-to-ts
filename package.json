{
	"name": "zod-to-ts",
	"version": "2.0.0",
	"type": "module",
	"description": "generate TypeScript types from your Zod schema",
	"author": {
		"name": "Sachin Raja",
		"url": "https://github.com/sachinraja"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/sachinraja/zod-to-ts.git"
	},
	"homepage": "https://github.com/sachinraja/zod-to-ts#readme",
	"bugs": {
		"url": "https://github.com/sachinraja/zod-to-ts/issues"
	},
	"zshy": "./src/index.ts",
	"files": [
		"dist"
	],
	"keywords": [
		"zod",
		"typescript",
		"generator"
	],
	"scripts": {
		"build": "zshy",
		"format": "biome format . --write",
		"lint": "pnpm run /^lint:/",
		"lint:biome": "biome check",
		"lint:types": "tsc",
		"test": "vitest"
	},
	"peerDependencies": {
		"typescript": "^5.0.0",
		"zod": "^3.25.0 || ^4.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.6",
		"@types/node": "24.7.2",
		"typescript": "5.9.3",
		"vite": "7.1.10",
		"vitest": "3.2.4",
		"zod": "4.1.12",
		"zshy": "0.4.4"
	},
	"packageManager": "pnpm@10.18.3",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.cts",
	"exports": {
		".": {
			"types": "./dist/index.d.cts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	}
}
